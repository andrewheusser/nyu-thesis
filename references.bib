	url = {http://www.sciencedirect.com/science/article/pii/S1053811910000959},
	doi = {10.1016/j.neuroimage.2010.01.073},
	shorttitle = {The pairwise phase consistency},
	abstract = {Oscillatory activity is a widespread phenomenon in nervous systems and has been implicated in numerous functions. Signals that are generated by two separate neuronal sources often demonstrate a consistent phase-relationship in a particular frequency-band, i.e., they demonstrate rhythmic neuronal synchronization. This consistency is conventionally measured by the {PLV} (phase-locking value) or the spectral coherence measure. Both statistical measures suffer from significant bias, in that their sample estimates overestimate the population statistics for finite sample sizes. This is a significant problem in the neurosciences where statistical comparisons are often made between conditions with a different number of trials or between neurons with a different number of spikes. We introduce a new circular statistic, the {PPC} (pairwise phase consistency). We demonstrate that the sample estimate of the {PPC} is a bias-free and consistent estimator of its corresponding population parameter. We show, both analytically and by means of numerical simulations, that the population statistic of the {PPC} is equivalent to the population statistic of the squared {PLV}. The variance and mean squared error of the {PPC} and {PLV} are compared. Finally, we demonstrate the practical relevance of the method in actual neuronal data recorded from the orbitofrontal cortex of rats that engage in a two-odour discrimination task. We find a strong increase in rhythmic synchronization of spikes relative to the local field potential (as measured by the {PPC}) for a wide range of low frequencies (including the theta-band) during the anticipation of sucrose delivery in comparison to the anticipation of quinine delivery.},
	pages = {112--122},
	number = {1},
	journaltitle = {{NeuroImage}},
	shortjournal = {{NeuroImage}},
	author = {Vinck, Martin and van Wingerden, Marijn and Womelsdorf, Thilo and Fries, Pascal and Pennartz, Cyriel M. A.},
	urldate = {2014-01-22},
	date = {2010-05-15},
	keywords = {Coherence, Coherence, Oscillation, Oscillation, Pairwise phase consistency, Pairwise phase consistency, Phase-locking, Phase-locking, Rhythmic, Rhythmic, synchronization, synchronization},
	file = {ScienceDirect Full Text PDF:/Users/andrewheusser/Library/Application Support/Zotero/Profiles/oqm03jss.default/zotero/storage/HZ6VWGS9/Vinck et al. - 2010 - The pairwise phase consistency A bias-free measur.pdf:application/pdf;ScienceDirect Snapshot:/Users/andrewheusser/Library/Application Support/Zotero/Profiles/oqm03jss.default/zotero/storage/GVZ5UJUT/S1053811910000959.html:text/html}
}

@article{hsieh_neural_2011,
	title = {Neural oscillations associated with item and temporal order maintenance in working memory},
	volume = {31},
	issn = {1529-2401},
	doi = {10.1523/JNEUROSCI.0828-11.2011},
	abstract = {The ability to retain information in working memory ({WM}) requires not only the active maintenance of information about specific items, but also the temporal order in which the items appeared. Although many studies have investigated the neural mechanisms of item maintenance, little is known about the neural mechanisms of temporal order maintenance in {WM}. Here, we used electroencephalography ({EEG}) to compare neural oscillations during {WM} tasks that required maintenance of item or temporal order information. Behavioral results revealed that accuracy and reaction times were comparable between the two conditions, suggesting that task difficulty was matched between the item and temporal order {WM} tasks. {EEG} analyses indicated that theta (5-7 Hz) oscillations over prefrontal sites were increased during temporal order maintenance, whereas alpha oscillations (9-12 Hz) over posterior parietal and lateral occipital sites were increased during item maintenance. The frontal theta enhancement was primarily evident in high performers on the order {WM} task, whereas the posterior alpha enhancement was primarily evident in high performers on the item {WM} task. These results support the idea that frontal theta and posterior alpha oscillations are differentially related to maintenance of item and temporal order information.},
	pages = {10803--10810},
	number = {30},
	journaltitle = {The Journal of neuroscience: the official journal of the Society for Neuroscience},
	shortjournal = {J. Neurosci.},
	author = {Hsieh, Liang-Tien and Ekstrom, Arne D and Ranganath, Charan},
	date = {2011-07-27},
	pmid = {21795532},
	pmcid = {PMC3164584},
	keywords = {Alpha Rhythm, Alpha Rhythm, Analysis of Variance, Analysis of Variance, Biological Clocks, Biological Clocks, Brain Mapping, Brain Mapping, Brain Waves, Brain Waves, Electroencephalography, Electroencephalography, Female, Female, Humans, Humans, Male, Male, Memory, Memory, Short-Term, Neuropsychological Tests, Neuropsychological Tests, Photic Stimulation, Photic Stimulation, Prefrontal Cortex, Prefrontal Cortex, Reaction Time, Reaction Time, Short-Term, Students, Students, Theta Rhythm, Theta Rhythm, Time Factors, Time Factors, Universities, Universities}
