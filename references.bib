	keywords = {80 and over, Activities of Daily Living, Activities of Daily Living, Aged, Aged, Aged, 80 and over, Alzheimer Disease, Alzheimer Disease, Attention, Attention, Comprehension, Comprehension, Concept Formation, Concept Formation, Female, Female, Humans, Humans, Male, Male, Mental Recall, Mental Recall, Pattern Recognition, Pattern Recognition, Visual, Retention (Psychology), Retention (Psychology), Semantics, Semantics, Serial Learning, Serial Learning, Video Recording, Video Recording, Visual}
}

@article{fortin_critical_2002,
	title = {Critical role of the hippocampus in memory for sequences of events},
